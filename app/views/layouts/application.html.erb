<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
  <title>Legacy</title>
  <%= stylesheet_link_tag "jquery-ui" %>
  <%= stylesheet_link_tag "bootstrap.minx", "application", "scaffold", 'template' %>

  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "jquery-ui", "jquery.watermark" %>
  <%= csrf_meta_tag %>
</head>

<body>

<div id="main_content">
  <div id="header">
    <div class="page-header">
      <%= render('shared/page_header') %>
    </div>
  </div>

  <div style="padding: 20px;">
    <div id="content ">
      <div>
        <%= render(:partial => "shared/flash_message") -%>
      </div>

      <%= render(:partial => "shared/left_panel") -%>

      <div id="content_main">
        <%= yield -%>
      </div>
    </div>
    <footer>
      <%= render('shared/footer') -%>
    </footer>
  </div>

</div>
</body>
</html>
