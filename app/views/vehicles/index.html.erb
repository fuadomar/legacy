<h1>Listing vehicles</h1>

<table>
  <tr>
    <th>Possession</th>
    <th>Manufacturrer</th>
    <th>Model</th>
    <th>Year</th>
    <th>Copy of title</th>
    <th>Location of original title</th>
    <th>Picture</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vehicles.each do |vehicle| %>
  <tr>
    <td><%= vehicle.possession_id %></td>
    <td><%= vehicle.manufacturrer %></td>
    <td><%= vehicle.model %></td>
    <td><%= vehicle.year %></td>
    <td><%= vehicle.copy_of_title %></td>
    <td><%= vehicle.location_of_original_title %></td>
    <td><%= vehicle.picture %></td>
    <td><%= vehicle.note %></td>
    <td><%= link_to 'Show', vehicle %></td>
    <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
    <td><%= link_to 'Destroy', vehicle, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

