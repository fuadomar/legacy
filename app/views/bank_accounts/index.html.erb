<h1>Listing bank_accounts</h1>

<table>
  <tr>
    <th>Financial account</th>
    <th>Name of bank</th>
    <th>Type of account</th>
    <th>Location of bank</th>
    <th>Last 4 digit of account</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bank_accounts.each do |bank_account| %>
  <tr>
    <td><%= bank_account.financial_account_id %></td>
    <td><%= bank_account.name_of_bank %></td>
    <td><%= bank_account.type_of_account %></td>
    <td><%= bank_account.location_of_bank %></td>
    <td><%= bank_account.last_4_digit_of_account %></td>
    <td><%= bank_account.note %></td>
    <td><%= link_to 'Show', bank_account %></td>
    <td><%= link_to 'Edit', edit_bank_account_path(bank_account) %></td>
    <td><%= link_to 'Destroy', bank_account, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bank account', new_bank_account_path %>
