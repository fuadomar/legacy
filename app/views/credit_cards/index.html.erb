<h1>Listing credit_cards</h1>

<table>
  <tr>
    <th>Financial account</th>
    <th>Card type</th>
    <th>Issuer</th>
    <th>Phone</th>
    <th>Last four digits of card</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @credit_cards.each do |credit_card| %>
  <tr>
    <td><%= credit_card.financial_account_id %></td>
    <td><%= credit_card.card_type %></td>
    <td><%= credit_card.issuer %></td>
    <td><%= credit_card.phone %></td>
    <td><%= credit_card.last_four_digits_of_card %></td>
    <td><%= credit_card.note %></td>
    <td><%= link_to 'Show', credit_card %></td>
    <td><%= link_to 'Edit', edit_credit_card_path(credit_card) %></td>
    <td><%= link_to 'Destroy', credit_card, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Credit card', new_credit_card_path %>
